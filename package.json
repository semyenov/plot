{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@sozdev/templater",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "yarn@3.6.3",
  "description": "Templater CLI",
  "author": "Alexander Semyenov <semyenov@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/semyenov/templater#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semyenov/templater.git"
  },
  "bugs": "https://github.com/semyenov/templater/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "tpl": "bin/tpl.mjs"
  },
  "scripts": {
    "dev": "esno src/index.ts",
    "build": "rollup --configPlugin typescript --config rollup.config.ts",
    "typecheck": "tsc --noEmit",
    "start": "node --experimental-modules --experimental-import-meta-resolve dist/esm/index.mjs",
    "schema": "typescript-json-schema --out schema.json --skipLibCheck src/lib/config.ts Config",
    "release": "yarn npm publish",
    "lint": "eslint --ext=ts,js,vue .",
    "clean": "yarn exec rm -rf dist"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/eslint-config-ts": "^0.41.0",
    "@antfu/utils": "^0.7.6",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/esm": "^3.2.0",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.5.6",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-antfu": "^0.41.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-yml": "^1.8.0",
    "esno": "^0.17.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^14.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "simple-git-hooks": "^2.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.60.0",
    "vue-eslint-parser": "^9.3.1",
    "yaml-eslint-parser": "^1.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --fix"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@clickhouse/client": "^0.2.2",
    "@observablehq/plot": "^0.6.10",
    "jsdom": "^22.1.0"
  }
}
